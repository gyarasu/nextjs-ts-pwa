{
  "name": "nextjs-ts-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon server/index.ts",
    "start:prd": "NODE_ENV=production node ./dist/server/index.js",
    "build": "yarn lint && next build && tsc -p ./server",
    "build:ci": "yarn lint && tsc --noEmit --incremental false",
    "lint": "eslint --fix -c ./.eslintrc.json './**/*.ts'",
    "start": "next start"
  },
  "dependencies": {
    "express": "4.17.1",
    "next": "9.4.4",
    "next-offline": "5.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  }
}
