{
  "name": "nextjs-ts-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon server/index.ts",
    "start:prd": "NODE_ENV=production node ./dist/server/index.js",
    "build": "yarn lint && next build && tsc -p ./server",
    "build:ci": "yarn lint && tsc --noEmit --incremental false",
    "lint": "eslint --fix -c ./.eslintrc.json './**/*.ts'",
    "start": "next start"
  },
  "dependencies": {
    "express": "4.17.1",
    "next": "9.3.4",
    "next-offline": "5.0.2",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@types/express": "4.17.4",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "lint-staged": "10.1.2",
    "nodemon": "2.0.2",
    "prettier": "2.0.2",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  }
}
