{
  "name": "nextjs-ts-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon server/index.ts",
    "start:prd": "NODE_ENV=production node ./dist/server/index.js",
    "build": "yarn lint && next build && tsc -p ./server",
    "build:ci": "yarn lint && tsc --noEmit --incremental false",
    "lint": "eslint --fix -c ./.eslintrc.json './**/*.ts'",
    "start": "next start"
  },
  "dependencies": {
    "express": "4.17.1",
    "next": "9.5.5",
    "next-offline": "5.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/express": "4.17.12",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "prettier": "2.3.1",
    "ts-node": "8.10.2",
    "typescript": "4.3.4"
  }
}
